{
  "name": "blakkisvuohi",
  "version": "4.0.0",
  "description": "A telegram bot that helps to track estimated blood alcohol concentration",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "build:watch": "rm -rf dist && tsc -w",
    "clean": "rm -rf node_modules coverage dist .nyc_output",
    "coverage": "nyc --exclude dist/test --reporter=html npm run test",
    "lint": "tslint 'src/**/*.ts' 'test/**/*.test.ts'",
    "start": "node dist/src/index.js",
    "start:dev": "tsc-watch --onSuccess 'node --inspect=0.0.0.0:5858 dist/src/index.js'",
    "test": "npm run build && mocha --exit --recursive dist/test/"
  },
  "engines": {
    "node": "8.9.4",
    "npm": "6.0.1"
  },
  "author": "Joonas Ulmanen",
  "license": "GPL-3.0",
  "dependencies": {
    "body-parser": "^1.18.3",
    "chart.js": "^2.7.2",
    "chartjs-node": "^1.7.1",
    "express": "^4.16.3",
    "loglevel": "^1.6.1",
    "newrelic": "^4.1.1",
    "node-pg-migrate": "^3.2.0",
    "node-telegram-bot-api": "^0.30.0",
    "pg": "^7.4.3",
    "prettier": "^1.13.4"
  },
  "devDependencies": {
    "@types/mocha": "^5.0.0",
    "@types/node": "^9.6.0",
    "mocha": "^5.2.0",
    "nyc": "^11.8.0",
    "tsc-watch": "^1.0.17",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.10.0",
    "tslint-eslint-rules": "^5.3.1",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.7.2"
  }
}

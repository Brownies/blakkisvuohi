{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules"
  ],
  "jsRules": {},
  "rules": {
    "semicolon": [true, "never"],
    "quotemark": [true, "single"],
    "indent": [false],
    "no-console": false,
    "interface-name": false,
    "object-literal-sort-keys": false,
    "max-classes-per-file": false,
    "ter-indent": [
        true,
        4
    ]
  }
}